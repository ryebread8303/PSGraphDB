public class GraphNode {
    public int ID;
    public Dictionary<string> Properties;
    public string[] Lables;
    public GraphRelation NextRelationship;
    public void GraphNode (int Sequence, string[] Types, Dictionary<string> Data) {
        ID = Sequence;
        Lables = Types;
        Properties = Data;
    }
}
public class GraphDatabase {
    public int CurrentID;
    public GraphNode[] NodeTable;
    public GraphRelation[] RelationTable;
}

public class GraphProperty {
    public string Key;
    public string Value;
}